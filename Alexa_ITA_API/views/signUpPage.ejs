                                <label class="col-sm-3 control-label" for="car_price">Price</label>
                                <div class="col-sm-9">
                                    <input id="car_price" class="form-control" ng-model="car_price"  name="car_price" type="text" placeholder="Price">
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-3 control-label" for="car_features">Features</label>
                                <div class="col-sm-9">
                                    <div ng-repeat="feature in features">
                                        <label>
                                            <input type="checkbox" checklist-model="car_features.features" checklist-value="feature"> 
                                            <div style=" margin: -20px 0px 0px 20px; padding: 5px;">{{feature}}</div>
                                        </label>
                                    </div>
                                </div>
                            </div>
                            <div class="col-md-2 col-md-offset-4">
                                <button ng-click="backToPage3()" class="btn btn-success btn-lg">Previous</button>
                            </div>
                            <div class="col-md-2 col-md-offset-1">
                                <button ng-click="page5()" class="btn btn-success btn-lg">Next</button>
                            </div>
                        </div>
                        <div id="ad" ng-show = "IsHPVisible">
                            <legend>Hotel Preferences</legend>
                            <div class="form-group">
                                <label class="col-sm-3 control-label" for="hotel_star_rating">Star Rating</label>
                                <div class="col-sm-9">
                                    <select id="hotel_star_rating" name="hotel_star_rating" ng-model="hotel_star_rating" class="form-control">
                                        <option value="" selected="selected">(please select a rating)</option>
                                        <option value="1">1</option>
                                        <option value="2">2</option>
                                        <option value="3">3</option>
                                        <option value="4">4</option>
                                        <option value="5">5</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-3 control-label" for="hotel_location">Location</label>
                                <div class="col-sm-9">
                                    <input id="hotel_location" class="form-control" ng-model="hotel_location"  name="hotel_location" type="text" placeholder="Location">
                                    <p class="help-block">Street address, city, country</p>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-3 control-label" for="hotel_price">Price</label>
                                <div class="col-sm-9">
                                    <input id="hotel_price" class="form-control" ng-model="hotel_price"  name="hotel_price" type="text" placeholder="Price">
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-3 control-label" for="food_type">Food Type</label>
                                 <div class="col-sm-9">
                                    <select id="food_type" name="food_type" ng-model="food_type" class="form-control">
                                        <option value="" selected="selected">(please select a type of food)</option>
                                        <option value="Meat">Meat</option>
                                        <option value="Turkey">Turkey</option>
                                        <option value="Chicken">Chicken and Other Poultry</option>
                                        <option value="Seafood">Seafood</option>
                                        <option value="Egg">Egg and Eggs Products</option>
                                        <option value="Diary">Milk, Cheese and Diary Products</option>
                                        <option value="Vegetarian">Fresh Fruits, Vegetables, and Juices (Vegetarian)</option>
                                    </select>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-3 control-label" for="food_cuisine">Food Cuisine</label>
                                <div class="col-sm-9">
                                    <select id="food_cuisine" name="food_cuisine" ng-model="food_cuisine" class="form-control">
                                        <option value="" selected="selected">(please select a cuisine)</option>
                                        <option value="American">American</option>
                                        <option value="British">British</option>
                                        <option value="Caribbean">Caribbean</option>
                                        <option value="Chinese">Chinese</option>
                                        <option value="French">French</option>
                                        <option value="Greek">Greek</option>
                                        <option value="Indian">Indian</option>
                                        <option value="Italian">Italian</option>
                                        <option value="Japanese">Japanese</option>
                                        <option value="Mediterranean">Mediterranean</option>
                                        <option value="Mexican">Mexican</option>
                                        <option value="Moroccan">Moroccan</option>
                                        <option value="Spanish">Spanish</option>
                                        <option value="Thai">Thai</option>
                                        <option value="Turkish">Turkish</option>
                                        <option value="Vietnamese">Vietnamese</option>
                                    </select>
                                </div>
                            </div>
                            <div class="col-md-2 col-md-offset-4">
                                <button ng-click="backToPage4()" class="btn btn-success btn-lg">Previous</button>
                            </div>
                            <div class="col-md-2 col-md-offset-1">
                                <button ng-click="page6()" class="btn btn-success btn-lg">Next</button>
                            </div>
                        </div>
                        <div id="pyd" ng-show = "IsPYDVisible">                
                            <legend>Payment Details</legend>
                            <div class="form-group">
                                <label class="col-sm-3 control-label" for="card-holder-name">Name on Card</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" name="card_holder_name" ng-model="card_holder_name" id="card-holder-name" placeholder="Card Holder's Name">
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-3 control-label" for="card-number">Card Number</label>
                                <div class="col-sm-9">
                                    <input type="text" class="form-control" name="card_number" ng-model="card_number" id="card-number" placeholder="Debit/Credit Card Number">
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-3 control-label" for="expiry-month">Expiration Date</label>
                                <div class="col-sm-9">
                                  <div class="row">
                                    <div class="col-xs-3">
                                      <select class="form-control col-sm-2" name="expiry_month" ng-model="expiry_month" id="expiry-month">
                                        <option>Month</option>
                                        <option value="01">Jan (01)</option>
                                        <option value="02">Feb (02)</option>
                                        <option value="03">Mar (03)</option>
                                        <option value="04">Apr (04)</option>
                                        <option value="05">May (05)</option>
                                        <option value="06">June (06)</option>
                                        <option value="07">July (07)</option>
                                        <option value="08">Aug (08)</option>
                                        <option value="09">Sep (09)</option>
                                        <option value="10">Oct (10)</option>
                                        <option value="11">Nov (11)</option>
                                        <option value="12">Dec (12)</option>
                                      </select>
                                    </div>
                                    <div class="col-xs-3">
                                      <select class="form-control" name="expiry_year" ng-model="expiry_year">
                                        <option value="13">2013</option>
                                        <option value="14">2014</option>
                                        <option value="15">2015</option>
                                        <option value="16">2016</option>
                                        <option value="17">2017</option>
                                        <option value="18">2018</option>
                                        <option value="19">2019</option>
                                        <option value="20">2020</option>
                                        <option value="21">2021</option>
                                        <option value="22">2022</option>
                                        <option value="23">2023</option>
                                      </select>
                                    </div>
                                  </div>
                                </div>
                            </div>
                            <div class="form-group">
                                <label class="col-sm-3 control-label" for="cvv">Card CVV</label>
                                <div class="col-sm-3">
                                    <input type="text" class="form-control" name="cvv" ng-model="cvv" id="cvv" placeholder="Security Code">
                                </div>
                            </div>
                            <div class="col-md-7 col-md-offset-3" style="margin-top: 10px">
                                <div class="col-md-2 col-md-offset-1">
                                    <button ng-click="backToPage5()" class="btn btn-ui btn-lg btn-block">Previous</button>
                                </div>
                                <div class="col-md-2 col-md-offset-4">
                                    <button type="submit" ng-click="signUp()" class="btn btn-ui btn-lg btn-block">Sign Up</button>
                                </div>
                            </div>
                        </div>
                    </fieldset>
                </form>
            </div>
        </div>
        <!-- Include all compiled plugins (below), or include individual files as needed -->
        <script src="../javascripts/lib/bootstrap.min.js"></script>
    </body>
</html>