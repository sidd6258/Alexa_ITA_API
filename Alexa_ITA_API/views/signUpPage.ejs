                    <fieldset style="min-height:100px;">
                        <div id="pd" ng-show = "IsPDVisible">
                            <legend>Personal Details</legend>
                            <form name= "form" novalidate>
                                <div class="form-group">
                                    <label class="col-sm-3 control-label" for="first-name">First Name</label>
                                    <span class="col-sm-4 asterisk_input">  </span> 
                                    <div class="col-sm-9">
                                        <input type="text" class="form-control" id="first_name" ng-model="first_name" placeholder="First Name" name = "first_name" ng-required="true">
                                        <p class="error" ng-show="form.firstName.$invalid && form.firstName.$touched">You must fill out your first name.</p>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-3 control-label" for="last-name">Last Name</label>
                                    <span class="col-sm-4 asterisk_input">  </span> 
                                    <div class="col-sm-9">
                                        <input type="text" class="form-control" id="last_name" ng-model="last_name" placeholder="Last Name" name = "last_name" ng-required="true">
                                        <p class="error" ng-show="form.lastName.$invalid && form.lastName.$touched">You must fill out your last name.</p>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-3 control-label" for="email">Email</label>
                                    <span class="col-sm-4 asterisk_input">  </span> 
                                    <div class="col-sm-9">
                                        <input type="email" class="form-control" id="email" ng-model="email" placeholder="Email" name = "email" ng-required="true">
                                        <p class="error" ng-show="form.email.$invalid && form.email.$touched">You must fill out correct email.</p>
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-sm-3 control-label" for="password">Password</label>
                                    <span class="col-sm-4 asterisk_input">  </span> 
                                    <div class="col-sm-9">
                                        <input type="password" class="form-control" id="password" ng-model="password" placeholder="Password" name = "password" ng-required="true">
                                        <p class="error" ng-show="form.password.$invalid && form.password.$touched">You must fill out your password.</p>
                                    </div>
                                </div>
                                <!-- Text input-->
                                <div class="form-group">
                                    <label class="col-sm-3 control-label" for="mobile">Mobile No.</label>
                                    <span class="col-sm-4 asterisk_input">  </span> 
                                    <div class="col-sm-9">
                                        <input id="mobile" name="mobile" placeholder="Mobile Number"  ng-model="mobile" class="form-control" type="text" ng-required="true">
                                        <p class="error" ng-show="form.mobile.$invalid && form.mobile.$touched">You must fill out your mobile number.</p>
                                    </div>
                                </div>

                                <!-- Appended Input-->
                                <div class="form-group">
                                  <label class="col-sm-3 control-label" for="dob">Date of Birth</label>
                                  <span class="col-sm-4 asterisk_input">  </span> 
                                    <div class="col-sm-9">
                                        <div class="input-append">
                                            <input id="dob" name="dob" class="form-control" ng-model="dob" placeholder="MM-DD-YYYY" type="text" ng-required="true">
                                            <p class="error" ng-show="form.dob.$invalid && form.dob.$touched">You must fill out your date of birth.</p>
                                        </div>
                                        <!-- <p class="help-block">Error</p> -->
                                    </div>
                                </div>

                                <!-- Multiple Radios (inline) -->
                                <div class="form-group">
                                    <label class="col-sm-3 control-label" for="gender">Gender</label>
                                    <span class="col-sm-4 asterisk_input">  </span> 
                                    <div class="col-sm-9" style="margin-left: 310px; margin-top: -25px;">
                                        <label class="radio inline" for="gender-0">
                                            <input name="gender" id="gender-0" value="Male" ng-model="gender" type="radio" ng-required="!gender">
                                            Male
                                        </label>
                                        <label class="radio inline" for="gender-1">
                                            <input name="gender" id="gender-1" value="Female" ng-model="gender" type="radio" ng-required="!gender">
                                            Female
                                        </label>
                                        <p class="error" ng-show="form.gender.$invalid && form.gender.$touched">You must select your gender.</p>
                                    </div>
                                </div>
                                <button ng-click="page2()"  ng-disabled="form.$invalid" class="col-md-4 col-md-offset-4 btn btn-success btn-lg">Next</button>
                            </form>
