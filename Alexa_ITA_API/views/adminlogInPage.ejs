        <div class="col-xs-12 back-pic">
            <center>
            <div class="row row-content" style="height: 100%;">
                <div id="login-widget wrapper" class="panel col-xs-3 col-xs-offset-5">
                    <form id="loginform form-signin" class="form-horizontal" role="form" novalidate>
                        <div class="panel-heading text-center">
                            <h5 class="text-uppercase form-signin-heading"><strong>Login to your account</strong></h5>
                        </div>
                        <div class="panel-body">
                            <label for="login-email">Email</label>
                            <div class="input-group">
                                <span class="input-group-addon"><i class="glyphicon glyphicon-user"></i></span>
                                <input id="email" type="text" class="form-control input-lg" ng-model="email" name="email" placeholder="email"  autofocus="">
                                <p class="error" style="margin-left: 20px;" ng-show="form.email.$invalid && form.email.$touched">You must fill out your email.</p>
                            </div>
                            <br/>
                            <label for="login-password">Password</label>
                            <div class="input-group">
                                <span class="input-group-addon"><i class="glyphicon glyphicon-lock"></i></span>
                                <input id="password" type="password" class="form-control input-lg" ng-model="password" name="password" placeholder="password">
                                <p class="error" style="margin-left: 20px;" ng-show="form.password.$invalid && form.password.$touched">You must fill out your password.</p>
                            </div>
                        </div>
                        <div class="panel-footer text-center">
                            <div class="login-btn">
                                <button type="submit" ng-click="logIn()" ng-disabled="form.$invalid" class="btn btn-ui btn-lg btn-block mrg-btm-10"><i class="fa fa-play-circle"></i>Log In</button>
                                <a href="#"><small>Forgot Username or Password?</small></a>
                            </div>
                            <!-- <a href="#" class="facebook"><i class="fa fa-lg fa-facebook-square"></i> Login with Facebook</a>        -->
                        </div>
                    </form>
                </div>
            </div>
            </center>
